<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/fragment_utils_bt_1"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="网络请求" />

    <Button
        android:id="@+id/fragment_utils_bt_2"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="获取前台线程的包名" />

    <Button
        android:text="BrowserActivity"
        android:id="@+id/fragment_utils_bt_web_view_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:text="WebViewRxActivity"
        android:id="@+id/fragment_utils_bt_web_view_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>
